<?xml version='1.0' encoding='UTF-8'?>
<module rename-to='project'>

  <!-- Inherit the core Web Toolkit stuff. -->
  <inherits name='com.google.gwt.user.User' />
  <inherits name='com.google.gwt.inject.Inject' />

  <!-- Other module inherits -->
  <inherits name='com.gwtplatform.mvp.Mvp' />

  <!-- Default warning for non-static, final fields enabled -->
  <set-property name="gwt.suppressNonStaticFinalFieldWarnings" value="false" />

  <generate-with class="pro.redsoft.iframework.client.generate.ModelGenerator">
    <when-type-assignable class="pro.redsoft.iframework.client.generate.Model" />
  </generate-with>

  <source path='client' />
  <source path='shared' />
</module>
