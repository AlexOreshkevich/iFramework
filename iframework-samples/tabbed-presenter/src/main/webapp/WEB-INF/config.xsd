<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.intertrust.ru" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  targetNamespace="http://www.intertrust.ru" elementFormDefault="qualified" attributeFormDefault="unqualified" jaxb:version="2.1">

  <xs:annotation>
    <xs:appinfo> CM4.X Standardized specification </xs:appinfo>
    <xs:documentation>
      https://docs.google.com/document/d/1ozEKsCCJUuv47P2NSdEGn8Se1FxqocWCazljemr19TI/edit#heading=h.v1170smuxtza
    </xs:documentation>
  </xs:annotation>

  <xs:complexType name="obj" />

  <!-- Argument description -->
  <xs:complexType name="argument">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1" />
          <xs:element name="value" type="xs:string" minOccurs="1" maxOccurs="1" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="arguments">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="arg" type="xsi:argument" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:element name="config" type="xsi:config" />

  <xs:simpleType name="bool">
    <xs:annotation>
      <xs:appinfo>
        <jaxb:class name="Boolean" ref="java.lang.Boolean" />
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="true|false" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Defining CDATA section -->
  <!-- http://www.w3.org/TR/2000/CR-xmlschema-2-20001024/#schema -->
  <xs:simpleType name="CDATA" id="CDATA">
    <xs:annotation>
      <xs:documentation source="http://www.w3.org/TR/xmlschema-2/#CDATA" xml:lang="en" />
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="replace" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Root type named config -->
  <xs:complexType name="config">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:all>
          <xs:element name="system" type="xsi:systemSettings" minOccurs="0" />
          <xs:element name="user" type="xsi:userSettings" minOccurs="0" />
          <xs:element name="logMessage" type="xs:string" minOccurs="0" />
        </xs:all>
        <xs:attribute name="version" type="xs:string" default="0.1-SNAPSHOT" />
        <xs:attribute name="schemaLocation" type="xs:string" use="optional" form="qualified" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Language settings -->
  <xs:complexType name="languageSettings">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:all>
          <xs:element name="defaultLocale" type="xs:string" minOccurs="0" default="ru" />
          <xs:element name="locales" type="xsi:listOfLocales" minOccurs="1" />
        </xs:all>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- List of concrete languages -->
  <xs:simpleType name="listOfLocales">
    <xs:list itemType="xs:language" />
  </xs:simpleType>

  <!-- System settings -->
  <xs:complexType name="systemSettings">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:all>
          <!-- Simple settings -->
          <xs:element name="entryPoint" type="xs:string" minOccurs="1" />
          <xs:element name="reference" type="xs:string" minOccurs="1" />
          <xs:element name="showTraceWindow" type="xs:boolean" />
          <xs:element name="useHistory" type="xs:boolean"  />
          <xs:element name="cookiesExpires" type="xs:int" minOccurs="1" default="1" />
          <xs:element name="ieRedirectUrl" type="xsi:CDATA" />
          <xs:element name="ieRedirectEnabled" type="xs:boolean" />
          <xs:element name="registrationEnabled" type="xs:boolean" />
          <xs:element name="registrationUrl" type="xs:string" minOccurs="1" />
          <xs:element name="enableMessageOnUncaughtException" type="xsi:bool" default="false" />
          <xs:element name="faviconPath" type="xs:string" />
          <xs:element name="logoImagePath" type="xs:string" />
          <xs:element name="logoImageWidth" type="xs:string" />
          <xs:element name="logoImageHeight" type="xs:string" />

          <xs:element name="themes" type="xsi:themes" minOccurs="1" />

          <!-- Complex settings -->
          <xs:element name="language" type="xsi:languageSettings" minOccurs="1" maxOccurs="1" />

          <!-- Email settings for error window -->
          <xs:element name="emailSettings" type="xsi:emailSettings" minOccurs="1" maxOccurs="1" />
        </xs:all>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="requestTimeout">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="entry" type="xsi:entry" minOccurs="1" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Email settings for error window -->
  <xs:complexType name="emailSettings">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:all>
          <xs:element name="mailHostName" type="xs:string" minOccurs="1" default="smtp.gmail.com" />
          <xs:element name="mailPortNumber" type="xs:string" minOccurs="1" default="587" />
          <xs:element name="mailAuthUser" type="xs:string" minOccurs="1" default="companymedia4@gmail.com" />
          <xs:element name="mailAuthPwd" type="xs:string" minOccurs="1" default="$$123456" />
          <xs:element name="mailFrom" type="xs:string" minOccurs="1" default="companymedia4@gmail.com" />
          <xs:element name="mailTo" minOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="mail" type="xs:string" minOccurs="1" maxOccurs="unbounded" default="companymedia4@gmail.com" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <!-- <xs:element name="mailTo" type="xsi:mailsTo" minOccurs="0" maxOccurs="unbounded" default="companymedia4@gmail.com" /> -->
          <xs:element name="mailDbug" type="xsi:bool" minOccurs="1" default="true" />
          <xs:element name="mailStarttls" type="xsi:bool" minOccurs="1" default="true" />
          <xs:element name="subject" type="xs:string" minOccurs="1" default="Error in CompanyMedia4" />
        </xs:all>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Resources description -->
  <xs:complexType name="resources">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="resource" type="xsi:resource" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Resource description -->
  <xs:complexType name="resource">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:all>
          <xs:element name="type" type="xsi:resource.type" minOccurs="1" maxOccurs="1" />
          <xs:element name="args" type="xsi:arguments" minOccurs="1" maxOccurs="1" />
        </xs:all>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Types of concrete resources are fixed -->
  <xs:simpleType name="resource.type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="title" />
      <xs:enumeration value="style" />
      <xs:enumeration value="base" />
      <xs:enumeration value="link" />
      <xs:enumeration value="meta" />
      <xs:enumeration value="script" />
      <xs:enumeration value="noscript" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Themes description -->
  <xs:complexType name="themes">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="default" type="xsi:theme" minOccurs="1" maxOccurs="1" />
          <xs:element name="theme" type="xsi:theme" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Types of concrete themes are fixed -->
  <xs:simpleType name="theme">
    <xs:restriction base="xs:string">
      <xs:enumeration value="default" />
      <xs:enumeration value="iridescent" />
      <xs:enumeration value="bright" />
    </xs:restriction>
  </xs:simpleType>

  <!-- User settings -->
  <xs:complexType name="userSettings">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:all>
          <xs:element name="margin" type="xs:string" default="0px" />
          <xs:element name="enableScrolling" type="xsi:bool" default="false" />
          <xs:element name="animation" type="xsi:animation" />
          <xs:element name="enableWindowClosingHandler" type="xsi:bool" default="false" />
          <xs:element name="components" type="xsi:components" minOccurs="1" />
          <xs:element name="requestTimeout" minOccurs="1" maxOccurs="1" type="xsi:requestTimeout" />
          <xs:element name="tokenConfig" type="xsi:tokenConfig" minOccurs="1" />
          <xs:element name="dialogConfig" type="xsi:dialogConfig" minOccurs="1" />
        </xs:all>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- animation configuration -->
  <xs:complexType name="animation">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:all>
          <xs:element name="enabled" type="xs:boolean" default="true" />
          <xs:element name="duration" type="xs:int" default="500" />
        </xs:all>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- components configuration -->
  <xs:complexType name="components">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:all>
          <xs:element name="previewComponent" type="xsi:previewConfig" minOccurs="1" />
          <xs:element name="dialogMediaView" type="xsi:dialogMediaTypeConfig" minOccurs="1" />
          <xs:element name="extSearchComponent" type="xsi:extSearchConfig" minOccurs="1" />
          <xs:element name="noticePanelComponent" type="xsi:noticePanelConfig" minOccurs="1" />
        </xs:all>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- componentForm configuration -->
  <xs:complexType name="componentForm">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="componentForm" type="xsi:componentForm" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute type="xsi:component.type" name="type" use="required" />
        <xs:attribute type="xsi:slot.type" name="slotType" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="tokenConfig">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="tokenConfigItem" type="xsi:tokenConfigItem" minOccurs="1" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="dialogConfig">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="dialog" type="xsi:dialogConfigItem" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- componentForm configuration -->
  <xs:complexType name="tokenConfigItem">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="componentFormConfig" type="xsi:componentFormConfig" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute type="xsi:token.type" name="type" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="dialogConfigItem">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="componentFormConfig" type="xsi:componentFormConfig" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute type="xsi:dialog.type" name="type" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Types of concrete tabs are fixed -->
  <xs:simpleType name="token.type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="default" />
      <xs:enumeration value="view" />
      <xs:enumeration value="frameset" />
      <xs:enumeration value="calendar" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="slot.type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ROOT_SLOT" />
      <xs:enumeration value="ACTION_BAR_SLOT" />
      <xs:enumeration value="SYSTEM_TREE_SLOT" />
      <xs:enumeration value="QUICK_TASK_SLOT" />
      <xs:enumeration value="REPORT_SLOT" />
      <xs:enumeration value="SPLITTER_SLOT" />
      <xs:enumeration value="TOP_LEFT_WRP_SLOT" />
      <xs:enumeration value="BOTTOM_RIGHT_WRP_SLOT" />
      <xs:enumeration value="GL_DLG_CONTENT" />
      <xs:enumeration value="NULL" />
    </xs:restriction>
  </xs:simpleType>

  <!-- component configuration -->
  <xs:complexType name="componentFormConfig">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="componentFormConfig" type="xsi:componentFormConfig" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="type" type="xsi:component.type" />
        <xs:attribute type="xsi:slot.type" name="slotType" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Types of concrete tabs are fixed -->
  <xs:simpleType name="component.type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="preview" />
      <xs:enumeration value="dialogPreview" />
      <xs:enumeration value="content" />
      <xs:enumeration value="systemTree" />
      <xs:enumeration value="actionBar" />
      <xs:enumeration value="splitLayoutPanel" />
      <xs:enumeration value="grid" />
      <xs:enumeration value="resources" />
      <xs:enumeration value="reports" />
      <xs:enumeration value="extSearch" />
      <xs:enumeration value="contentTab" />
      <xs:enumeration value="detailsTab" />
      <xs:enumeration value="universalView" />
      <xs:enumeration value="universalViewDialog" />
      <xs:enumeration value="calendarDialog" />
      <xs:enumeration value="gridSettingsDialog" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="dialog.type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="searchDialog" />
      <xs:enumeration value="calendarDialog" />
      <xs:enumeration value="gridSettingsDialog" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Preview configuration -->
  <xs:complexType name="previewConfig">
    <xs:complexContent>
      <xs:extension base="xsi:componentFormConfig">
        <xs:sequence>
          <xs:element name="mediaType" type="xsi:mediaTypeConfig" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- MediavType View (in dialogs fe.) configuration -->
  <xs:complexType name="dialogMediaTypeConfig">
    <xs:complexContent>
      <xs:extension base="xsi:componentFormConfig">
        <xs:sequence>
          <xs:element name="mediaType" type="xsi:mediaTypeConfig" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- ExtSearch configuration -->
  <xs:complexType name="extSearchConfig">
    <xs:complexContent>
      <xs:extension base="xsi:componentFormConfig">
        <xs:sequence>
          <xs:element name="fieldOrder" type="xsi:parameterConfig" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="parameterConfig">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="field" type="xs:string" minOccurs="1" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="noticePanelConfig">
    <xs:complexContent>
      <xs:extension base="xsi:componentFormConfig">
        <xs:sequence>
          <xs:element name="noticePanelParameters" type="xsi:noticePanelParameters" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="noticePanelParameters">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="timeout" type="xs:int" minOccurs="0" maxOccurs="1" />
          <xs:element name="visible" type="xs:boolean" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <!-- Dedicated mediaType configuration -->
  <xs:complexType name="mediaTypeConfig">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="contentType" type="xs:string" minOccurs="1" />
          <xs:element name="header" type="xsi:previewHeader" minOccurs="0" maxOccurs="1" />
          <xs:element name="tab" type="xsi:tab" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="defaultSelectedTab" type="xs:string" default="detailsTab" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Preview header configuration -->
  <xs:complexType name="previewHeader">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="row" type="xsi:previewHeaderRow" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="withControl" type="xs:boolean" default="true" />
        <xs:attribute name="withExecution" type="xs:boolean" default="true" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Preview header row configuration -->
  <xs:complexType name="previewHeaderRow">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="headerElement" type="xsi:headerElem" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="visible" type="xs:boolean" default="true" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Preview headerElement configuration -->
  <xs:complexType name="headerElem">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:attribute name="type" type="xsi:headerElem.type" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Types of concrete tabs are fixed -->
  <xs:simpleType name="headerElem.type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="mediaTypeElement" />
      <xs:enumeration value="documentTypeElement" />
      <xs:enumeration value="specialTagElement" />
      <xs:enumeration value="numberDateElement" />
      <xs:enumeration value="signatureElement" />
      <xs:enumeration value="termElement" />
      <xs:enumeration value="controlButton" />
      <xs:enumeration value="executionButton" />
      <xs:enumeration value="accessElement" />
      <xs:enumeration value="statusElement" />
      <xs:enumeration value="contractStatusElement" />
      <xs:enumeration value="contractExecElement" />
      <xs:enumeration value="agendaElement" />
      <xs:enumeration value="meetingTimeElement" />
      <xs:enumeration value="meetingDateElement" />
      <xs:enumeration value="meetingDateTimeElement" />
      <xs:enumeration value="meetingDateTransferElement" />
      <xs:enumeration value="meetingFormElement" />
      <xs:enumeration value="numberElement" />
      <xs:enumeration value="placeElement" />
      <xs:enumeration value="dateElement" />
      <xs:enumeration value="protocolElement" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Dedicated tab configuration -->
  <xs:complexType name="tab">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="section" type="xsi:tab.section" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="subtab" type="xsi:tab.subtab" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="type" type="xsi:tab.type" />
        <xs:attribute name="subType" type="xsi:tab.subType" default="default" />
        <xs:attribute name="alias" type="xs:string" use="optional" />
        <xs:attribute name="helpId" type="xs:string" use="optional" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Types of concrete tabs are fixed -->
  <xs:simpleType name="tab.type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="contentTab" />
      <xs:enumeration value="detailsTab" />
      <xs:enumeration value="docTasksTab" />
      <xs:enumeration value="relatedTab" />
      <xs:enumeration value="discussionTab" />
      <xs:enumeration value="historyTab" />
      <xs:enumeration value="previewHeader" />
      <xs:enumeration value="costPaymentTab" />
      <xs:enumeration value="contractStructTab" />
      <xs:enumeration value="cyprusTab" />
      <xs:enumeration value="caseCalendarTab" />
      <!--<xs:enumeration value="caseStructureTab" /> -->
      <xs:enumeration value="caseDocumentsTab" />
      <xs:enumeration value="caseWorkTab" />
      <xs:enumeration value="relatedTabWithOutLeftMenu" />
      <xs:enumeration value="informationMaterialsTab" />
      <xs:enumeration value="executionTab" />
      <xs:enumeration value="classifiersTab" />
      <xs:enumeration value="reportTab" />
      <xs:enumeration value="headCommentTab" />
      <xs:enumeration value="folderTab" />
      <xs:enumeration value="reportTab" />
      <xs:enumeration value="directoriesTab" />
      <xs:enumeration value="accessTab" />
      <xs:enumeration value="approvalTempTab" />
      <xs:enumeration value="reviewTempTab" />
      <xs:enumeration value="caseMembersTab" />
      <xs:enumeration value="caseTimelineTab" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Types of concrete tabs are fixed -->
  <xs:simpleType name="tab.subType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="default" />
      <xs:enumeration value="meetingMinutesTab" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Types of sections -->
  <xs:complexType name="tab.section">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:sequence>
          <xs:element name="element" type="xsi:tab.details.element" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="alias" type="xs:string" />
        <xs:attribute name="open" type="xs:boolean" use="optional" default="true" />
        <xs:attribute name="styleElementCaption" type="xs:string" use="optional" default="" />
        <xs:attribute name="styleElementContainer" type="xs:string" use="optional" default="" />
        <xs:attribute name="widthElement" type="xs:string" use="optional" default="" />
        <xs:attribute name="isSendingFormWidget" type="xs:boolean" use="optional" default="false" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Types of subtabs -->
  <xs:complexType name="tab.subtab">
    <xs:complexContent>
      <xs:extension base="xsi:obj">
        <xs:attribute name="alias" type="xs:string" />
        <xs:attribute name="select" type="xs:boolean" use="optional" default="false" />
        <xs:attribute name="visible" type="xs:boolean" use="optional" default="true" />
        <xs:attribute name="helpId" type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <!-- Types of sections -->
  <xs:complexType name="tab.details.element">
    <xs:complexContent>
